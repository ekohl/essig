atmel {
	parameters {
		gprs 32;
		ram 1024;
	}

	registers {
		/* -- Status Register (SREG) -- */
		SREG; /* Status register */
		C; /* Carry flag */
		Z; /* Zero flag */
		N; /* Negative flag */
		V; /* Two's complement overflow indicator */
		S; /* N^V, For signed tests */
		H; /* Half Carry Flag */
		T; /* Transfer bit used by BLD and BST instructions */

		/* -- Rest -- */
		PC; /* Program counter */
	}

	mov Rd, Rr {
		Rd = Rr;
	}

	and Rd, Rr {
		/* 0 <= d <= 31, 0 <= r 31 */
		Rd = Rd & Rr;
		PC = PC + 1;
		S = N ^ V;
		V = 0;
		N = R7;
		Z = !R7 & !R6 & !R5 & !R4 & !R3 & !R2 & !R1 & !R0;
	}
}
